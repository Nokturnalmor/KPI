QTabWidget::pane { /* The tab widget frame */
    border-top: 2px solid rgba(195, 52, 69, 152);
}

QTabWidget::tab-bar {
    left: 5px; /* move to the right by 5px */
}

/* Style the tab using the tab sub-control. Note that
    it reads QTabBar _not_ QTabWidget */
QTabBar::tab {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0  rgb(99, 99, 99), stop: 0.4  rgb(49, 49, 49),
                                stop: 0.5  rgb(49, 49, 49), stop: 1.0  rgb(99, 99, 99));
    border: 1px solid rgb(233, 78, 13);
	color: rgb(230, 230, 230);
    border-bottom-color: rgba(195, 52, 69, 152); /* same as the pane color */
    border-top-left-radius: 5px;
    border-top-right-radius: 9px;
    min-width: 12ex;
    padding: 5px;
}



QTabBar::tab:selected {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
								stop: 0  rgb(89, 89, 89), stop: 0.4  rgb(39, 39, 39),
                                stop: 0.5  rgb(49, 49, 49), stop: 1.0  rgb(99, 99, 99));
 border-color: rgb(195, 52, 69);
    border-bottom-color: rgba(195, 52, 69, 252); /* same as pane color */
}

QTabBar::tab:!selected {
    margin-top: 5px; /* make non-selected tabs look smaller */
	 background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
								stop: 0  rgb(129, 129, 129), stop: 0.4  rgb(59, 59, 59),
                                stop: 0.5  rgb(59, 59, 59), stop: 1.0  rgb(129,129, 129));
}
QTabBar::tab:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
								stop: 0  rgb(89, 89, 89), stop: 0.4  rgb(49, 49, 49),
                                stop: 0.5  rgb(49, 49, 49), stop: 1.0  rgb(99, 99, 99));
}
